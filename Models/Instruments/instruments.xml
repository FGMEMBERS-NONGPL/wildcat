<?xml version="1.0"?>

<PropertyList>

	
 
 
 <!-- Switches -->
  
  <model>
		<name>Landing lights</name>
    <path>Aircraft/wildcat/Models/Instruments/switches/landing-lights.xml</path>
    <offsets> 
      <x-m>-2.983</x-m>
   <y-m>0.05</y-m>
   <z-m>-0.396</z-m>
<pitch-deg>-10</pitch-deg>
    </offsets>
	</model>
	
	<animation>
    <type>select</type>
    <object-name>NoseGearLandinglight</object-name>
    <object-name>Taxi-Light-Left</object-name>
         
    <condition>
      <equals>
        <property>controls/switches/landing-lights-switch</property>
        <value> 1 </value>
      </equals>
    </condition>
  </animation> 	
  
  
